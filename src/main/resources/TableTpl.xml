<?xml version="1.0" encoding="UTF-8"?>
<root>
    <table name="web_data_profil">
        <!-- id=数据库字段 ref=json值 <column>where 条件</column>-->
        <!-- datas 非数据库字段，是嵌套子表判断条件是包含[list]-->
        <column id="Rpt_ID" ref="Id"/>
        <column id="Rpt_Typ" ref="type"/>
        <column id="Rpt_Nm" ref="rptNm"/>
        <column id="Rpt_Prd" ref="rtpPrd"/>
        <column id="Src_Web_Addr" ref="srcWeb"/>
        <column id="Src_Desc" ref="srcDesc"/>
    </table>
    <!-- 参展人员信息/活动信息 -->
    <!-- 子表配置参数 json字段 子表list表名 关联主表字段-->
    <table name="GATE_EXPO_AUDI_INFO">
        <column id="id" ref="id">id = roleType + id</column>
        <column id="datas" ref="[list]GATE_Prtc_Act_Info" linkId="id"/>
    </table>

    <table name="GATE_Prtc_Act_Info">
        <column id="act_Id" ref="id">id = id</column>
    </table>

    <!-- 停车场信息 -->
    <table name="PARK_PARKING_LOT">
        <column id="Park_Lot_Regn_ID" ref="Park_Lot_Regn_ID">Park_Lot_Regn_ID = Park_Lot_Regn_ID</column>
    </table>

    <!-- 车辆位置消息/历史 -->
    <table name="BUS_VEHIC_LCTN_MSG;BUS_VEHIC_LCTN_MSG_HIS">
        <column id="OwnRoute_ID" ref="OwnRoute.Id"></column>
        <column id="OwnRoute_Name" ref="OwnRoute.Name"></column>
        <column id="OwnRoute_Code" ref="OwnRoute.Code"></column>
        <column id="RunRoute_ID" ref="RunRoute.Id"></column>
        <column id="RunRoute_Name" ref="RunRoute.Name"></column>
        <column id="RunRoute_Code" ref="RunRoute.Code"></column>
    </table>





</root>





